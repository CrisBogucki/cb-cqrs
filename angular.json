{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "cb-cqrs": {
      "projectType": "library",
      "root": "projects/cb-cqrs",
      "sourceRoot": "projects/cb-cqrs/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/cb-cqrs/tsconfig.lib.json",
            "project": "projects/cb-cqrs/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/cb-cqrs/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/cb-cqrs/src/test.ts",
            "tsConfig": "projects/cb-cqrs/tsconfig.spec.json",
            "karmaConfig": "projects/cb-cqrs/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/cb-cqrs/tsconfig.lib.json",
              "projects/cb-cqrs/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "app": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/app",
      "sourceRoot": "projects/app/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/app",
            "index": "projects/app/src/index.html",
            "main": "projects/app/src/main.ts",
            "polyfills": "projects/app/src/polyfills.ts",
            "tsConfig": "projects/app/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/app/src/favicon.ico",
              "projects/app/src/assets"
            ],
            "styles": [
              "projects/app/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/app/src/environments/environment.ts",
                  "with": "projects/app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "app:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/app/src/test.ts",
            "polyfills": "projects/app/src/polyfills.ts",
            "tsConfig": "projects/app/tsconfig.spec.json",
            "karmaConfig": "projects/app/karma.conf.js",
            "assets": [
              "projects/app/src/favicon.ico",
              "projects/app/src/assets"
            ],
            "styles": [
              "projects/app/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/app/tsconfig.app.json",
              "projects/app/tsconfig.spec.json",
              "projects/app/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/app/e2e/protractor.conf.js",
            "devServerTarget": "app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "app:serve:production"
            }
          }
        }
      }
    },
    "app-with-lib": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/app-with-lib",
      "sourceRoot": "projects/app-with-lib/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/app-with-lib",
            "index": "projects/app-with-lib/src/index.html",
            "main": "projects/app-with-lib/src/main.ts",
            "polyfills": "projects/app-with-lib/src/polyfills.ts",
            "tsConfig": "projects/app-with-lib/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/app-with-lib/src/favicon.ico",
              "projects/app-with-lib/src/assets"
            ],
            "styles": [
              "projects/app-with-lib/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/app-with-lib/src/environments/environment.ts",
                  "with": "projects/app-with-lib/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "app-with-lib:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "app-with-lib:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app-with-lib:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/app-with-lib/src/test.ts",
            "polyfills": "projects/app-with-lib/src/polyfills.ts",
            "tsConfig": "projects/app-with-lib/tsconfig.spec.json",
            "karmaConfig": "projects/app-with-lib/karma.conf.js",
            "assets": [
              "projects/app-with-lib/src/favicon.ico",
              "projects/app-with-lib/src/assets"
            ],
            "styles": [
              "projects/app-with-lib/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/app-with-lib/tsconfig.app.json",
              "projects/app-with-lib/tsconfig.spec.json",
              "projects/app-with-lib/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/app-with-lib/e2e/protractor.conf.js",
            "devServerTarget": "app-with-lib:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "app-with-lib:serve:production"
            }
          }
        }
      }
    },
    "cqrs": {
      "projectType": "library",
      "root": "projects/cqrs",
      "sourceRoot": "projects/cqrs/src",
      "prefix": "cb",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "projects/cqrs/tsconfig.lib.json",
            "project": "projects/cqrs/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/cqrs/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/cqrs/src/test.ts",
            "tsConfig": "projects/cqrs/tsconfig.spec.json",
            "karmaConfig": "projects/cqrs/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/cqrs/tsconfig.lib.json",
              "projects/cqrs/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "cb-cqrs"
}
